sky:
  datasource:
    driver-class-name: com.mysql.cj.jdbc.Driver
    host: localhost
    port: 3306
    database: sky_take_out
    username: your_username
    password: your_password

  redis:
    host: localhost
    port: 6379
    # 如果 Redis 有密码，取消下面的注释并设置密码
    # password: your_redis_password
    database: 10

  alioss:
    endpoint: oss-cn-shanghai.aliyuncs.com
    access-key-id: your_access_key_id
    access-key-secret: your_access_key_secret
    bucket-name: your_bucket_name

  wechat:
    appid: your_wechat_appid
    secret: your_wechat_secret
    mock: true # 默认可设为 true 进行支付模拟，正式环境请改为 false
    mchid: your_mchid
    mch-serial-no: your_mch_serial
    private-key-file-path: D:/cert/apiclient_key.pem
    api-v3-key: your_api_v3_key
    we-chat-pay-cert-file-path: D:/cert/wechatpay_cert.pem
    notify-url: https://your.domain.com/notify/paySuccess
    refund-notify-url: https://your.domain.com/notify/refundSuccess



